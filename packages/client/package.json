{
	"name": "@todo-graphql/client",
	"version": "1.0.0",
	"private": true,
	"scripts": {
		"<---BUILD--->": "-------------------------------------------------------------------------------------------------",
		"build": "craco build",
		"<---DEV--->": "---------------------------------------------------------------------------------------------------",
		"dev": "env-cmd -f ../../envs/.env craco start",
		"<---LINTER--->": "------------------------------------------------------------------------------------------------",
		"lint": "eslint \"src/**/{*.ts,*.tsx}\" --fix",
		"<---FORMATTER--->": "---------------------------------------------------------------------------------------------",
		"format": "prettier --write \"src/**/*.ts\" \"src/**/*.tsx\"",
		"<---GENERATE_GQL--->": "------------------------------------------------------------------------------------------",
		"generate:gql": "env-cmd -f ../../envs/.env graphql-codegen --config codegen.js"
	},
	"dependencies": {
		"@todo-graphql/common": "1.0.0",
		"@apollo/client": "^3.3.20",
		"@craco/craco": "^6.0.0",
		"@hookform/resolvers": "^2.5.1",
		"@material-ui/core": "^4.11.4",
		"@material-ui/icons": "^4.11.2",
		"class-transformer": "^0.4.0",
		"class-validator": "^0.13.1",
		"clsx": "^1.1.1",
		"craco-alias": "^3.0.1",
		"date-fns": "^2.22.1",
		"graphql": "^15.5.1",
		"react": "^17.0.2",
		"react-dom": "^17.0.2",
		"react-hook-form": "^7.6.6",
		"react-router-dom": "^5.2.0",
		"react-scripts": "4.0.3",
		"react-toastify": "^7.0.4"
	},
	"devDependencies": {
		"@graphql-codegen/cli": "^1.21.5",
		"@graphql-codegen/introspection": "^1.18.2",
		"@graphql-codegen/typescript": "^1.22.3",
		"@graphql-codegen/typescript-operations": "^1.18.2",
		"@graphql-codegen/typescript-react-apollo": "^2.2.7",
		"@types/react": "^17.0.0",
		"@types/react-dom": "^17.0.0",
		"@types/react-router-dom": "^5.1.7",
		"@typescript-eslint/eslint-plugin": "^4.12.0",
		"@typescript-eslint/parser": "^4.6.1",
		"env-cmd": "^10.1.0",
		"eslint": "^7.17.0",
		"eslint-config-airbnb": "^18.2.1",
		"eslint-config-prettier": "^7.1.0",
		"eslint-import-resolver-alias": "^1.1.2",
		"eslint-plugin-import": "^2.22.1",
		"eslint-plugin-jsx-a11y": "^6.4.1",
		"eslint-plugin-prettier": "^3.3.1",
		"eslint-plugin-react": "^7.22.0",
		"eslint-plugin-react-hooks": "^4.2.0",
		"prettier": "^2.2.1",
		"typescript": "^4.1.2"
	},
	"browserslist": {
		"production": [
			">0.2%",
			"not dead",
			"not op_mini all"
		],
		"development": [
			"last 1 chrome version",
			"last 1 firefox version",
			"last 1 safari version"
		]
	}
}
